{
    "name": "tuscanicz/doctrine-data-applier",
    "type": "library",
    "description": "Symfony bundle for Doctrine Migrations of data using doctrine entities.",
    "keywords": ["doctrine", "data", "migrations"],
    "homepage": "https://github.com/tuscanicz/doctrine-data-applier",
    "license": "MIT",
    "authors": [
        {
            "name": "Petr Bechyne",
            "email": "tuscanicz@gmail.com"
        }
    ],
    "require": {
        "symfony/http-kernel": "~2.5|~3.0",
        "symfony/config": "~2.5|~3.0",
        "symfony/dependency-injection": "~2.5|~3.0",
        "curl/curl": "~1.2",
        "symfony/framework-bundle": "~2.5|~3.0",
        "symfony/console": "~2.5|~3.0",
        "symfony/translation": "~2.5|~3.0",
        "doctrine/doctrine-migrations-bundle": "^1.1",
        "doctrine/orm": "^2.5.6",
        "kstefan/enum": "^1.0",
        "monolog/monolog": "^1.22"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.3.0"
    },
    "autoload": {
        "psr-4": {"DataApplier\\": "src/DataApplier"}
    }
}
